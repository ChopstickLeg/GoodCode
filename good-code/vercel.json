{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*).css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
    "functions": {
      "api/*.go": { "runtime": "vercel-go@0.5.1" }
    }
}
